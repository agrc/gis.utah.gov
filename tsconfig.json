{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@images/solutions/*": [
        "src/images/solutions/*"
      ],
      "@images/sgid/*": [
        "src/images/product-sgid/*"
      ],
      "@images/about/*": [
        "src/images/pillar-about/*"
      ],
      "@utils/*": [
        "src/utilities/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@models/*": [
        "src/pages/*"
      ],
      "@partials/*": [
        "src/pages/_partials/*"
      ],
    }
  }
}
