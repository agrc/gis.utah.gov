    "node_modules",
    "scripts/cleanser.py",
    "scripts/cleanser.pyc",
    "scripts/datatable.py",
    "scripts/datatable.pyc",
    "scripts/tests.py",
    "scripts/tests.pyc",
    "Gemfile.lock",
    "Gemfile",
    "GruntFile.js",
    "bower.json",
    "package.json",
    "*.bak"
